
@{
    ViewBag.Title = "Transferencias";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    a.disabled {
  pointer-events: none;
  cursor: default;
}

    .uniqueClassName {
        text-align: center;
    }
</style>


<script>
  UsuariosSystem = @Helios.Cont.Web.Helpers.JavaScriptConvert.SerializeObject(Model.UsuariosSistema);
</script>

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Logistica
            <small>Registro de transferencias entre almacenes</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Logistica</a></li>
            <li><a href="#">Almacen</a></li>
            <li class="active">Transferencias</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                <!-- /.box -->
                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title">Listado por período</h3>
                    </div>

                    <table class="table table-responsive" border="0">
                        <tr>
                            <td width="25%">
                                <select id="cboMes" class="form-control">
                                    <option id="00" value="-1">-Mes-</option>
                                    <option id="01" value="01">ENERO</option>
                                    <option id="02" value="02">FEBRERO</option>
                                    <option id="03" value="03">MARZO</option>
                                    <option id="04" value="04">ABRIL</option>
                                    <option id="05" value="05">MAYO</option>
                                    <option id="06" value="06">JUNIO</option>
                                    <option id="07" value="07">JULIO</option>
                                    <option id="08" value="08">AGOSTO</option>
                                    <option id="09" value="09">SETIEMBRE</option>
                                    <option id="10" value="10">OCTUBRE</option>
                                    <option id="11" value="11">NOVIEMBRE</option>
                                    <option id="12" value="12">DICIEMBRE</option>
                                </select>
                            </td>
                            <td width="25%">
                                <input type="number" name="Anio" id="textAnio" class="form-control" value="@DateTime.Now.Year" />
                            </td>
                            <td width="10">
                                <button id="btnConsultarTransfer" class="btn btn-success">Consultar</button>
                            </td>
                            <td>
                                <input id="btnNuevaTransfer" type="button" class="btn btn-primary" value="Nuevo" onclick="location.href='@Url.Action("CreateWarehouseTransfer", "Logistics")'" />
                            </td>
                        </tr>
                    </table>

                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="table-responsive">
                            <table id="dtTransfer" class="table table-responsive table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th>ID</th>
                                        <th>Fecha operación</th>
                                        <th>Usuario</th>
                                        <th>Tipo doc.</th>
                                        <th>Número</th>
                                        <th>Almacén partida</th>
                                        <th>Almacén llegada</th>
                                        <th>Items transferidos</th>
                                        <th>Estado</th>
                                        <th>&nbsp;</th>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                            <div />

                        </div>
                        <!-- /.box-body -->
                    </div>
                    <!-- /.box -->
                </div>
                <!-- /.col -->
            </div>
        </div>
        <!-- /.row -->

        <div id='myModal' class='modal fade' tabindex="-1">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div id='myModalContent'></div>
                </div>
            </div>

        </div>

    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
@section Scripts
{
    <link href="~/Content/bootstrape-select/bootstrap-select.min.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrape-select/bootstrap-select.min.js"></script>
    <script src="~/Scripts/Logistic/jLogisticTransferStore.js"></script>
    <script src="~/Scripts/Logistic/jLogisticTransfer.js"></script>
    <script type="text/javascript">

        $(document).ready(function () {

        });

    </script>

    @*Scripts*@

    @*  @Scripts.Render("~/bundles/bootstrap-select")*@

    @*Styles*@
    @*   @Styles.Render("~/Content/Bootstrap-Select/css")*@
}