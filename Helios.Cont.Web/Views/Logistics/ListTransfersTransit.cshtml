

@{
    ViewBag.Title = "Transferencias en tránsito";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    a.disabled {
        pointer-events: none;
        cursor: default;
    }

    .uniqueClassName {
        text-align: center;
    }
</style>


<script>
  UsuariosSystem = @Helios.Cont.Web.Helpers.JavaScriptConvert.SerializeObject(Model.UsuariosSistema);
</script>

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Logistica
            <small>transferencias entre almacenes</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Logistica</a></li>
            <li><a href="#">Almacén</a></li>
            <li class="active">Transferencias</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                <!-- /.box -->
                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title">Pendientes de recepción</h3>
                    </div>                 

                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="table-responsive">
                            <table id="dtTransferTransit" class="table table-responsive table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th>ID</th>
                                        <th>Fecha operación</th>
                                        <th>Usuario</th>
                                        <th>Tipo doc.</th>
                                        <th>Número</th>
                                        <th>Almacén partida</th>
                                        <th>Almacén llegada</th>
                                        <th>Items transferidos</th>
                                        <th>Estado</th>
                                        <th>&nbsp;</th>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                            <div />

                        </div>
                        <!-- /.box-body -->
                    </div>
                    <!-- /.box -->
                </div>
                <!-- /.col -->
            </div>
        </div>
        <!-- /.row -->

        <div id='myModal' class='modal fade' tabindex="-1">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div id='myModalContent'></div>
                </div>
            </div>

        </div>

    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
@section Scripts
{
    <link href="~/Content/bootstrape-select/bootstrap-select.min.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrape-select/bootstrap-select.min.js"></script>
    <script src="~/Scripts/Logistic/jLogisticTransferStore.js"></script>
    <script src="~/Scripts/Logistic/jLogisticTransfer.js"></script>
    <script type="text/javascript">

        $(document).ready(function () {
            GetTransfersTransit();
        });

    </script>

    @*Scripts*@

    @*  @Scripts.Render("~/bundles/bootstrap-select")*@

    @*Styles*@
    @*   @Styles.Render("~/Content/Bootstrap-Select/css")*@
}